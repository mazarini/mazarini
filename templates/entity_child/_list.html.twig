<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Slug</th>
			<th>actions</th>
		</tr>
	</thead>
	<tbody>
		{% for entity_child in entity_children %}
			<tr>
				<td>{{ entity_child.id }}</td>
				<td>{{ entity_child.slug }}</td>
				<td>{{ entity_child.parent }}</td>
				<td>
					<a href="{{ path('app_entity_child_show', {'id': entity_child.id}) }}">show</a>
					<a href="{{ path('app_entity_child_edit', {'id': entity_child.id}) }}">edit</a>
				</td>
			</tr>
		{% else %}
			<tr>
				<td colspan="3">no records found</td>
			</tr>
		{% endfor %}
	</tbody>
</table>
